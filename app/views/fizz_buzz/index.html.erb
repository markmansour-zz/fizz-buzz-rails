<h1>Continuously Deploy with AWS CodePipeline</h1>

<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>Number</th>
      <th>What to say</th>
    </tr>
  </thead>
  <tbody>
    <% 1.upto 16 do |i| %>
    <tr>
      <td>
	<%= i -%>
      </td>
      <td>
	<%= @fb.current %>

	<% if @fb.fizz_buzz? %>
	<span class="label label-success">FizzBuzz</span>
	<% elsif @fb.fizz? %>
	<span class="label label-primary">Fizz</span>
	<% elsif @fb.buzz? %>
	<span class="label label-info">Buzz</span>
	<% end %>
      </td>
      <%= @fb.next %>  
    </tr>
    <% end %>
  </tbody>
</table>

  <li></li>
</ol>
